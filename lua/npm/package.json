    "dev-amp": "webpack --watch --config ./config/webpack.base.js --env.prod --progress --debug",
    "lint": "eslint -c config/linters/.eslintrc.js --ext js --ext jsx components && stylelint 'src/**/*.scss' --config config/linters/.stylelintrc.json",
    "fusion": "fusion",
    "start": "fusion start",
    "test": "npx jest --coverage --watch",
    "todo": "npx leasot -x --reporter table 'components/**/*.js*' 'src/**/*.*css' > ./reports/todo.txt"
  },
